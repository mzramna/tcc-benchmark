{
    "dados_log":{
        "level":10,
        "name":"monitoramento_dados",
        "logstash_data":{
            "host":"192.168.0.116",
            "port":5000
            
    }
    },
    "monitor_data":{
        "cpu_times":{
            "columns":[],
            "parameters":{

            }
            },
            "cpu_percent":{
                "parameters":{"percpu":true,"interval":0.0},
                "columns":[]
            },
            "virtual_memory":{
                "parameters":{},
                "columns":["available","used","free"]
            },
            "swap_memory":{
                "parameters":{},
                "columns":["used","free","percent"]
            },
            "disk_usage":{
                "parameters":{
                    "path":"/"
                    },
                    "columns":["total","used","free","percent"]
                },
            "disk_io_counters":{
                "parameters":{
                    "perdisk":true
                    ,"nowrap":false
                    },
                    "columns":["read_time","write_time","busy_time","read_bytes","write_bytes"],
                    "array":["sda","sdb","sdc","sdd"]
                },
            "net_io_counters":{
                "parameters":{
                    "pernic":false,
                    "nowrap":false},
                    "columns":["bytes_sent","bytes_recv","packets_sent","packets_recv"]
                }
        },
    "docker_data":{
        "client_connect":{
        "base_url":"tcp://127.0.0.1:2375",
        "version":"auto"
        },
        "containers":[
            "alpine_no_daemon-mariadb-1",
            "alpine_no_daemon-postgres-1"
        ]
    }
}